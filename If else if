#include <stdio.h>

int main()
{
  int num;
  printf("Enter your marks:");
  scanf("%d",&num);
  if(num>=85)
  {
    printf("Grade-A");
  }
  else if(num>=70)
  {
    printf("Grade-B");
  }
  else if(num>=55)
  {
    printf("Grade-C");
  }
  else if(num>=40)
  {
    printf("Grade-D");
  }
  else 
  {
    printf("Grade-F");
  }
  return 0;
}
